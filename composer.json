{
  "name": "plaisio/exception",
  "description": "PhpPlaisio: Exception",
  "keywords": [
    "PhpPlaisio",
    "Exception"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "setbased/exception": "^2.3.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Exception\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
